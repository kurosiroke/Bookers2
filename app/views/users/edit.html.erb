<h2>User info</h2>
<%= form_with model: current_user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"Name" %>
  <label for="inputImage">Image</label>
  <%= f.file_field :profile_image, placeholder: "profile_image", accept: "image/*" %>
  <label for="user introduction">introduction</label>
  <%= f.

  <%= f.submit "Update Users" %>
<% end %>